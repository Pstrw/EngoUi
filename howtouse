local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Pstrw/EngoUi/main/engoui.lua", true))()

local main = library:CreateMain("name", "", Enum.KeyCode.LeftAlt) -- keybind

local tab = main:CreateTab("Combat")
  
tab:CreateLabel("Reach")
  
tab:CreateToggle("toggle", function(value)
  print(value)
end)

tab:CreateSlider("slider", 0, 90, function(value)
  print(value)
end)

tab:CreateButton("button", function()
  print("button pressed"
end)

-- other functions are self explanitory based on the current ones listed 
